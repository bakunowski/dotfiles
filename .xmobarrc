Config
    { font = "xft:RobotoMono Nerd Font:pixelsize=18:antialias=true:hinting=true"
    , additionalFonts   = [ "xft:FontAwesome:pixelsize=18:antialias=true:hinting=true" ]
    , allDesktops       = True
    , bgColor           = "#151515"
    , fgColor           = "#808080"
    , alpha             = 255
    , overrideRedirect  = True
    , commands           = [
        Run Wireless "wlp2s0"
            [ "-a", "l"
            , "-x", "-"
            , "-t", "<fc=#e1a3ee><fn=1>\xf1eb</fn> <essid></fc>"
            , "-L", "50"
            , "-H", "75"
            -- , "-l", "#dc322f" -- red
            , "-l", "#e1a3ee" -- violet
            , "-n", "#e1a3ee" -- violet
            , "-h", "#e1a3ee" -- violet
            ] 10
        , Run ThermalZone 0
            ["-t",      " <fc=#808080>Temp:<temp>C</fc>"
            ] 10
        , Run Memory
            [ "-t", "<fc=#808080>Mem:<available></fc>"
            ] 60
        , Run Battery
            [ "-t", " <fc=#acc267><acstatus></fc>"
            , "-L", "20"
            , "-H", "85"
            , "-l", "#acc267"
            , "-n", "#acc267"
            , "-h", "#acc267"
            , "--" -- battery specific options
            -- discharging status
            , "-o"  , "<fn=1>\xf241</fn> <left>% "
            -- AC "on" status
            , "-O"  , "<fn=1>\xf1e6</fn> <left>% "
            -- charged status
            , "-i"  , "<fn=1>\xf1e6</fn> <left>% "
            , "--off-icon-pattern", "<fn=1>\xf1e6</fn>"
            , "--on-icon-pattern", "<fn=1>\xf1e6</fn>"
            ] 60
        , Run Date "<fc=#6fc2ef><fn=1>\xf073</fn> %a %d %b %Y</fc>  <fc=#d0d0d0><fn=1>ï€—</fn> %H:%M:%S</fc>" "date" 10
        , Run Cpu
            [ "-t","<fn=1>\xf085</fn> C/M <vbar>"
            --, "-p", "2"
            , "-L", "40"
            , "-H", "60"
            , "-l", "#586e75"
            , "-h", "#dc322f" -- red
            ] 10
        , Run Network "eth0"
            [ "-t", " <fc=#6c71c4><fn=1>\xf065</fn> ETH<rxipat></fc>"
            ] 10
        , Run DynNetwork
            [ "-t", "<fc=#6c71c4>| <dev></fc>"
            ] 10
        , Run Com "status-vol" [] "volume" 5
        , Run Com "status-net" [] "net" 10
        , Run Com "status-keyboard" [] "kb" 1
        , Run Com "screencast" ["status"] "screencast" 10
        , Run Com "status-gmusic" [] "nowplaying" 10
        , Run StdinReader
        , Run Com "vpn.sh" [] "vpn" 25
        ]
        , sepChar            = "%"
        , alignSep           = "}{"
        --, template           = " %StdinReader% }{ %nowplaying% %screencast% %cpu%%memory%   %net%   %default:Master%   %battery%   %date%   %KBFI%   %kb%  "
        --, template           = " %StdinReader% }{ %nowplaying% %screencast% %cpu%%memory%   %net%   %volume%   %battery%   %date%   %kb%  "
        --, template           = "%StdinReader% }{ %cpu%%memory%   %statusnet% %wlp4s0wi%%enp0s31f6%   %default:Master%   %battery%   %date%   %KBFI%            "
        --, template           = "%StdinReader% }{ %wlp4s0wi% %default:Master% %multicpu% %cpufreq% %memory% %battery% %date% %KBFI%            "
        , template           = " %StdinReader% }{ %wlp2s0wi%%vpn% %thermal0% %memory% %kb% %battery% %date% "
    }
